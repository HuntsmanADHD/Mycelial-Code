# Test with struct literals
network struct_test {
  frequencies {
    test_sig {
      a: u32
      b: u32
      c: u32
      d: string
    }
  }

  types {
    struct MyStruct {
      field1: u32
      field2: u32
      field3: string
      field4: string
    }
  }

  hyphae {
    hyphal TestAgent {
      state {
        x: u32
        data: MyStruct
      }

      on rest {
        state.data = MyStruct {
          field1: 1,
          field2: 2,
          field3: "hello",
          field4: "world"
        }

        emit test_sig {
          a: 10,
          b: 20,
          c: 30,
          d: "test"
        }
      }
    }
  }
}
